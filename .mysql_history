CREATE TABLE rattaremont(
id INT NOT NUL AUTO_INCREMENT PRIMARY KEY,
kylastusaeg DATETIME,
klient VARCHAR(30),
kontakt VARCHAR(50),
teenus TEXT,
teenindaja VARCHAR(30),
summa DECIMAL (8, 2),
makstud DATETIME
);
CREATE TABLE rattaremont( id INT NOT NUL AUTO_INCREMENT PRIMARY KEY, kylastusaeg DATETIME, klient VARCHAR(30), kontakt VARCHAR(50), teenus TEXT, teenindaja VARCHAR(30), summa DECIMAL (8, 2), makstud DATETIME );
CREATE TABLE rattaremont(
id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
kylastusaeg DATETIME,
klient VARCHAR(30),
kontakt VARCHAR(50),
teenus TEXT,
teenindaja VARCHAR(30),
summa DECIMAL (8, 2),
makstud DATETIME
);
